{
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.js"
        },
        {
            "src": "/client/dist/angular_survey/(.*)",
            "dest": "dist"
        }
    ]
}
