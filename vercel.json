{
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.js"
        }
    ],
    "rewrites": [
        {
            "source": "/client/dist/angular_survey/(.*)",
            "destination": "/dist"
        }
    ]
}
